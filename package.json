    "jasmine-core": "^2.3.4",
    "karma": "^0.12.35",
    "karma-chrome-launcher": "^0.1.8",
    "karma-firefox-launcher": "^0.1.4",
    "karma-jasmine": "^0.3.5",
    "karma-sourcemap-loader": "^0.3.4",
    "karma-webpack": "^1.5.0",
    "minifier": "^0.6.2"
  }
